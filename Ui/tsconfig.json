{
	"compilerOptions": {
		"diagnostics": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [ "es2017", "dom" ],
		"listFiles": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"outDir": "wwwroot",
		"removeComments": false,
		"rootDir": "wwwroot",
		"sourceMap": true,
		"suppressImplicitAnyIndexErrors": true,
		"target": "es2017"
	},
	"exclude": [
		"node_modules"
	]
}
